Entry-Name: .content

get-button New | click -arrow
get-menu -path "Project..." | click
with [get-window "New Project"] {
    get-tree | select "General/Project"
    get-button "Next >" | click
    get-editbox -after [get-label "Project name:"] | set-text "acceleo-test"
    get-button Finish | click
}
get-view "Navigator" | get-tree | select "acceleo-test" | get-menu -path "Import..." | click
with [get-window Import] {
    get-tree | select "SNCA Import/Import SNCA XML File"
    get-button "Next >" | click
}
get-window "File Import Wizard" | get-button Finish | click
with [get-view Navigator | get-tree] {
    select "acceleo-test/SNCA.hitro" | get-menu -path "Acceleo Model to Text/Generate Codegen" | click
    select "acceleo-test/src-gen/SNCA-ALL.md" | select "acceleo-test/src-gen/SNCA-ALL.md" | get-menu 
        -path "Open With/Text Editor" | click
}
get-editor "SNCA-ALL.md" | get-text-viewer | set-caret-pos 23 29
