<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated hitro -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="de.sernet.eclipse.hitro.presentation.HitroModelWizardID"
            name="%_UI_HitroModelWizard_label"
            class="de.sernet.eclipse.hitro.presentation.HitroModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/HitroModelFile.gif">
         <description>%_UI_HitroModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated hitro -->
      <editor
            id="de.sernet.eclipse.hitro.presentation.HitroEditorID"
            name="%_UI_HitroEditor_label"
            icon="icons/full/obj16/HitroModelFile.gif"
            extensions="hitro"
            class="de.sernet.eclipse.hitro.presentation.HitroEditor"
            contributorClass="de.sernet.eclipse.hitro.presentation.HitroActionBarContributor">
      </editor>
   </extension>

</plugin>
